<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="genre_tests_in_groups">

    <test name="genre_tests">
        <groups>
            <run>
                <include name="genreGroup"/>
            </run>
        </groups>
        <classes>
            <class name="GenreTest"/>
        </classes>
    </test>

    <test name="genre_tests_without_precondition">
        <groups>
            <run>
                <include name="withoutPreconditionGroup"/>
            </run>
        </groups>
        <classes>
            <class name="GenreTest"/>
        </classes>
    </test>
</suite>